project 'Spy.xcodeproj'
 
use_frameworks!

target 'Example' do
  platform :macos, '10.10'
  pod 'SwiftLint'
end

target 'Spy_macOS' do
  platform :macos, '10.10'
  pod 'SwiftLint'

  # Pods for Spy

  target 'SpyTests' do
    # Pods for testing
  	inherit! :search_paths
  	pod 'Sourcery'
  end

end